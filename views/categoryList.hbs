<div class="row">
    <div class="col-md-12">
        <h3>{{title}}</h3>
    </div>
    <div class="col-md-12">
        <table class="table table-condensed table-hover table-bordered">
            <thead>
            <tr>
                <th>编号</th>
                <th>分类名称</th>
                <th>创建时间</th>
                <th>更新时间</th>
                <td>修改</td>
                <td>删除</td>
            </tr>
            </thead>
            <tbody>
            {{#each categorys}}
                <tr>
                    <td><a href="./detail/{{id}}">{{id}}</a></td>
                    <td>{{name}}</td>
                    <td>{{createTime}}</td>
                    <td>{{updateTime}}</td>
                    <td><a href="./admins?id={{id}}">修改</a></td>
                    <td><a class="delete" data-id="{{id}}" href="javascript:void(0);">删除</a></td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    <div class="col-md-2">
        <a href="./" class="btn btn-block btn-danger">Add</a>
    </div>
</div>